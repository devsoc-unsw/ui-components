{
  "name": "csesoc-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,mdx}'",
    "format": "prettier --write src/**/*.{ts,tsx,mdx}",
    "lint": "eslint --ext src/**/*{.ts,.tsx,.mdx}",
    "lint:fix": "eslint --ext src/**/*{.ts,.tsx,.mdx} --fix"
  },
  "dependencies": {
    "@csesoc/ui-components": "workspace:*",
    "@stitches/react": "^1.2.8",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@chromatic-com/storybook": "^1",
    "@storybook/addon-actions": "8.1.1",
    "@storybook/addon-essentials": "8.1.1",
    "@storybook/addon-interactions": "8.1.1",
    "@storybook/addon-links": "8.1.1",
    "@storybook/react": "8.1.1",
    "@storybook/react-vite": "^8.1.1",
    "@storybook/test": "8.1.1",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.2.1",
    "babel-loader": "9.1.3",
    "eslint": "9.2.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "3.3.3",
    "storybook": "8.1.1",
    "tsconfig": "workspace:*",
    "typescript": "5.4.5",
    "vite": "5.2.11"
  }
}
